def cadastro(nome,email,login,senha,tipo):
    return {"Nome":nome,"e-mail":email,"login":login,"senha":senha,"(Professor|Aluno)":tipo}

def adicionar(p,l):
    l.append(p)
    return l

def mostra(l):
    for x in l:
        for k,v in x.items():
            print("{}:{}".format(k,v))

def alunonota(nome,nota):
    return {"Nome": nome, "nota": nota}

def alunofalta(nome,f):
    return {"Nome": nome, "Faltas": f}

def login(l,s):
    return {"Login":l,"Senha":s}
